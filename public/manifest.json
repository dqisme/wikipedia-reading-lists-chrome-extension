{
  "name": "Wikipedia Reading Lists",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Read Wikipedia articles saved from Wikipediaâ€™s iOS and Android apps",
  "homepage_url": "https://github.com/dqisme/wikipedia-reading-lists-chrome-extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Wikipedia Reading Lists",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "cookies",
    "https://*.wikipedia.org/*",
    "https://*.wikivoyage.org/*"
  ]
}